compilation_database_dir: .
output_directory: puml
diagrams:
  ProfSpline_class:
    type: class
    generate_packages: true
    glob:
      - src/ProfSpline/*.cxx
      - src/RwCalculators/GReWeightProfessor.cxx
    using_namespace: genie::rew
    include:
      context:
        - r: 'genie::rew::.*'
        - r: 'Professor::.*'
        - r: 'genie::Algorithm'
    exclude:
      elements:
        - genie::rew::GSyst
        - genie::rew::GSystInfo
        - genie::rew::GSystSet
        - genie::rew::EGSyst
        - genie::rew::GReWeightI
        - genie::EventRecord
        - genie::Registry
        - genie::AlgId
        - Professor::IpolError
        - Professor::ParamPointsError
      namespaces:
        - std
      element_types:
        - enum
  ProfSpline_sequence:
    type: sequence
    generate_packages: true
    generate_return_types: true
    glob:
      - src/ProfSpline/*.cxx
      - src/RwCalculators/GReWeightProfessor.cxx
    using_namespace: genie::rew
    include:
      namespaces:
        - genie::rew
        - Professor
        - genie
    exclude:
      elements:
        - genie::rew::GSyst
        - genie::rew::GSystInfo
        - genie::rew::GSystSet
        - genie::rew::EGSyst
        - genie::rew::GReWeightI
        - genie::Messenger
        - Professor::IpolError
        - Professor::ParamPointsError
      context:
        - genie::Messenger
    start_from:
      - function: genie::rew::GReWeightProfessor::Initialize(std::string)
      - function: genie::rew::GReWeightProfessor::CalcWeight(const genie::EventRecord &)
